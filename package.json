  "main": "lib/djweb.common.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "node build/build.js",
    "release": "npm run build:lib && node auto/release.js -p -d && npm run updateJournal && npm run upload",
    "lint": "dj lint",
    "lint:js": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "fix:js": "eslint --fix --ext .js,.vue src test/unit test/e2e/specs",
    "lint:html": "htmlhint --config .htmlhintrc src/**/*.vue",
    "lint:style": "stylelint --config .stylelintrc.json src/**/*.{vue,css,scss,less}",
    "fix:style": "stylelint --config .stylelintrc.json **/*.{vue,css,scss,less} --fix",
    "updateSource": "node auto/updateSourceProject.js",
    "updateJournal": "node auto/updateJournal.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "build:common": "webpack --config build/webpack.common.js",
    "build:css": "cross-env TARGET=djweb webpack --config build/webpack.css.js",
    "build:theme": "cross-env TARGET=theme webpack --config build/webpack.css.js",
    "build:common-for-browers": "webpack --config build/webpack.common.browers.js",
    "build:lib": "npm run build:common && npm run build:common-for-browers && npm run build:css && npm run build:theme",
    "upload": "node auto/upload.js"
